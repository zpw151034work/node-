#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../_clean-css@3.4.28@clean-css/bin/cleancss" "$@"
else 
  exec node  "$basedir/../_clean-css@3.4.28@clean-css/bin/cleancss" "$@"
fi
